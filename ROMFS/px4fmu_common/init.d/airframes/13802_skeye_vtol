#!nsh
#
# @name Skeye Octorotor + Tailsitter
# 
# @type VTOL Octo Coax Tailsitter
# @class VTOL
#
# @output MAIN1 motor 1
# @output MAIN2 motor 2
# @output MAIN3 motor 3
# @output MAIN4 motor 4
# @output MAIN5 motor 5
# @output MAIN6 motor 6
# @output MAIN7 motor 7
# @output MAIN8 motor 8
#
# @maintainer Marc Rieben <mrieben@gmail.com>
#

sh /etc/init.d/rc.vtol_defaults

set MAV_TYPE 23

set MIXER octo_+_cox_w

set PWM_OUT 12345678

param set PWM_MAX 1950
param set PWM_MIN 1075

param set VT_MOT_COUNT 8
param set VT_TYPE 0

param set BAT_N_CELLS 4
param set BAT_V_DIV 12.10917568
param set BAT_V_CHARGED 4.20

param set CBRK_IO_SAFETY 22027
param set TEL_FRSKY_CONFIG 102
